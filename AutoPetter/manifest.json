{
  "Name": "AutoPetter",
  "Author": "Brogio",
  "Version": "1.1.0",
  "Description": "Automatically pets animals and pets with configurable friendship multiplier.",
  "UniqueID": "Brogio.AutoPetter",
  "EntryDll": "AutoPetter.dll",
  "MinimumApiVersion": "3.0.0",
  "UpdateKeys": [ "Nexus:27203" ],
  "ConfigSchema": {
    "PetAnimals": {
      "AllowValues": "true, false",
      "Default": true
    },
    "PetPets": {
      "AllowValues": "true, false",
      "Default": true
    },
    "FriendshipMultiplier": {
      "AllowValues": {
        "min": 0.1,
        "max": 5.0,
        "interval": 0.1
      },
      "Default": 1.0
    }
  },
  "Dependencies": [
    {
      "UniqueID": "spacechase0.GenericModConfigMenu",
      "IsRequired": false
    }
  ]
}